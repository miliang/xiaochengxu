.tandh,.time{
   display: flex;
  flex-wrap: wrap;
  width: 20%;
}

.time text:nth-child(2),.tandh text:nth-child(2){
   color: #828282;
  font-size: 28rpx;
}

.pandb{
  color: red
}

.sign{
  color: red;
  font-size: 20rpx;
}

.buy{
  color:#26d62f;
  font-size: 28rpx;
  outline: 1px solid #26d62f;
  padding: 6rpx;
  height: 40rpx;
  margin-left: 20rpx;
}
